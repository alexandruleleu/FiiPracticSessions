{
    "name": "session1",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "firebase": "^5.8.6",
        "less-watch-compiler": "^1.13.0",
        "npm-run-all": "^4.1.5",
        "react": "^16.8.3",
        "react-dom": "^16.8.3",
        "react-scripts": "2.1.5",
        "semantic-ui-css": "^2.4.1",
        "semantic-ui-react": "^0.85.0"
    },
    "scripts": {
        "build-css": "less-watch-compiler --run-once --main-file=index.less src/assets/less/ src/assets/less/",
        "watch-css": "npm run build-css && less-watch-compiler --main-file=index.less src/assets/less/ src/assets/less/",
        "start-js": "react-scripts start",
        "start": "npm-run-all -p watch-css start-js",
        "build-js": "react-scripts build",
        "build": "npm-run-all build-css build-js",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "precommit": "lint-staged"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
